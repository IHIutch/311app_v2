{
  "name": "buffalo-311",
  "version": "1.0.0",
  "description": "Buffalo 311 Nuxt",
  "author": "IHIutch",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/server/index.js --watch server",
    "start": "cross-env NODE_ENV=production node src/server/index.js",
    "build": "nuxt build",
    "copy": "npm run copy:nuxt && npm run copy:static",
    "copy:nuxt": "mkdir -p public/.nuxt && cp -r functions/.nuxt/dist/* public/.nuxt",
    "copy:static": "cp -r src/static/* public",
    "firebase:build": "npm run build && npm run copy && cd functions && npm i",
    "firebase:start": "firebase serve"
  },
  "dependencies": {
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/firebase": "^5.2.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@nuxtjs/sentry": "^4.1.3",
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.15.0",
    "cross-env": "^7.0.2",
    "dayjs": "^1.8.28",
    "express": "^4.16.4",
    "firebase": "^7.15.1",
    "geolib": "^3.3.1",
    "nuxt": "^2.12.2",
    "nuxt-leaflet": "0.0.21",
    "vue-feather-icons": "^5.0.0",
    "vue-unicons": "^2.2.1"
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2"
  }
}
